#!/bin/bash
export NWCHEM_EXE=/home/hvandam/nwchem-1-codar/bin/LINUX64/nwchem
mpirun -np 1 $NWCHEM_EXE nwc_gbe_631g_rhf.nw     | tee nwc_gbe_631g_rhf.out
